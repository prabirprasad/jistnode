{{!-- Recently Added Users Tables --}}
<div class="container">
  <div class="row mb-5">
    <div class="col-xl-10 col-lg-10 col-md-8 ml-auto">
      <div class="row pt-md-3 mt-md-3 mb-5">
        <div class="col-12">

          {{!-- Heading --}}
          <h3 class="text-light text-center mb-3 py-4">Recently Added Users</h3>

          {{!-- All Users Details --}}
          <table class="table table-dark table-hover table-striped text-center">

            {{!-- Table head --}}
              <thead>

                {{!-- Details --}}
                <tr class="text-light">
                  <tr>

                    {{!-- User Name --}}
                    <th scope="col">User Name</th>

                    {{!-- Full name --}}
                    <th scope="col">Full Name</th>

                    {{!-- Course --}}
                    <th scope="col">Course</th>

                    {{!-- Department --}}
                    <th scope="col">Department</th>

                    {{!-- Semester --}}
                    <th scope="col">Semester</th>

                    {{!-- Actions --}}
                    <th scope="col">Action</th>
                  </tr>
                </tr>
            </thead>

            {{!-- Table Body --}}
            <tbody>
                {{#each users}}
                <tr>
                  <td>{{this.username}}</td>
                  <td>{{this.fullname}}</td>
                  <td>{{this.course}}</td>
                  <td>{{this.department}}</td>
                  <td>{{this.semester}}</td>
                  <td><a href="/admin/user/{{this._id}}" class=""> <i class="fa fa-eye"></i> View</a></td>
                </tr>
                {{/each}}
            </tbody>

          </table>

        </div>
      </div>
    </div>
  </div>
</div>



